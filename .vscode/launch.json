    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "envFile": "${workspaceFolder}/.env",
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/greyhoundDashboard/manage.py"
        }
        ,
        {
            "name": "Python Debugger: Service Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tooling/service_generation.py",
            // "args": [
            //     "--base_class_file", "${workspaceFolder}/greyhoundDashboard/services/base_service.py",
            //     "--base_class_name", "BaseService"
            // ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Python Debugger: awesome_selfhosted_parser",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tooling/awesome_selfhosted_parser.py",
            // "args": [
            //     "--base_class_file", "${workspaceFolder}/greyhoundDashboard/services/base_service.py",
            //     "--base_class_name", "BaseService"
            // ],
            "envFile": "${workspaceFolder}/.env"
        },
    ]
}